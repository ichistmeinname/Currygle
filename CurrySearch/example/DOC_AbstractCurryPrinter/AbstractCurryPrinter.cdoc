(CurryInfo (ModuleInfo "AbstractCurryPrinter" "Martin Engelke, Bernd Brassel, Michael Hanus, Marion Mueller,\n        Parissa Sadeghi" "A pretty printer for AbstractCurry programs.\n\nThis library defines a function \"showProg\" that shows\nan AbstractCurry program in standard Curry syntax.\n\n") [(FunctionInfo "showProg" (FuncType (TCons ("AbstractCurry","CurryProg") []) (TCons ("Prelude","[]") [(TCons ("Prelude","Char") [])])) "AbstractCurryPrinter" "Shows an AbstractCurry program in standard Curry syntax.\nThe export list contains the public functions and the\ntypes with their data constructors (if all data constructors are public),\notherwise only the type constructors.\nThe potential comments in function declarations are formatted as\ndocumentation comments.\n" False ConflictFR),(FunctionInfo "showTypeDecls" (FuncType (TCons ("Prelude","[]") [(TCons ("AbstractCurry","CTypeDecl") [])]) (TCons ("Prelude","[]") [(TCons ("Prelude","Char") [])])) "AbstractCurryPrinter" "Shows a list of AbstractCurry type declarations in standard Curry syntax.\n" False KnownRigid),(FunctionInfo "showTypeDecl" (FuncType (TCons ("AbstractCurry","CTypeDecl") []) (TCons ("Prelude","[]") [(TCons ("Prelude","Char") [])])) "AbstractCurryPrinter" "Shows an AbstractCurry type declaration in standard Curry syntax.\n" False KnownFlex),(FunctionInfo "showTypeExpr" (FuncType (TCons ("Prelude","Bool") []) (FuncType (TCons ("AbstractCurry","CTypeExpr") []) (TCons ("Prelude","[]") [(TCons ("Prelude","Char") [])]))) "AbstractCurryPrinter" "Shows an AbstractCurry type expression in standard Curry syntax.\nIf the first argument is True, the type expression is enclosed\nin brackets.\n" False ConflictFR),(FunctionInfo "showFuncDecl" (FuncType (TCons ("AbstractCurry","CFuncDecl") []) (TCons ("Prelude","[]") [(TCons ("Prelude","Char") [])])) "AbstractCurryPrinter" "Shows an AbstractCurry function declaration in standard Curry syntax.\n" False UnknownFR),(FunctionInfo "showExpr" (FuncType (TCons ("AbstractCurry","CExpr") []) (TCons ("Prelude","[]") [(TCons ("Prelude","Char") [])])) "AbstractCurryPrinter" "Shows an AbstractCurry expression in standard Curry syntax.\n" False UnknownFR),(FunctionInfo "showPattern" (FuncType (TCons ("AbstractCurry","CPattern") []) (TCons ("Prelude","[]") [(TCons ("Prelude","Char") [])])) "AbstractCurryPrinter" [] False ConflictFR)] [])