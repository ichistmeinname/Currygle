(CurryInfo (ModuleInfo "FlatXML" "Michael Hanus" "This library contains functions to convert FlatCurry programs\ninto corresponding XML expressions and vice versa.\nThis can be used to store Curry programs in a way independent\nfrom PAKCS or to use the PAKCS back end by other systems.\n\nThe library provides the following functions:\n<PRE>\n\"flatCurry2XmlFile\": transform a FlatCurry program into XML file\n\"flatCurry2Xml\":     transform a FlatCurry program into XML expression\n\"xmlFile2FlatCurry\": read an XML file and return the FlatCurry program\n\"xml2FlatCurry\":     transform an XML expression into a FlatCurry program\n</PRE>\n\n") [(FunctionInfo "flatCurry2XmlFile" (FuncType (TCons ("Flat","Prog") []) (FuncType (TCons ("Prelude","[]") [(TCons ("Prelude","Char") [])]) (TCons ("Prelude","IO") [(TCons ("Prelude","()") [])]))) "FlatXML" "Transforms a FlatCurry program term into a corresponding XML file.\n" False UnknownFR),(FunctionInfo "flatCurry2Xml" (FuncType (TCons ("Flat","Prog") []) (TCons ("XML","XmlExp") [])) "FlatXML" "Transforms a FlatCurry program term into a corresponding XML expression.\n" False KnownFlex),(FunctionInfo "xmlFile2FlatCurry" (FuncType (TCons ("Prelude","[]") [(TCons ("Prelude","Char") [])]) (TCons ("Prelude","IO") [(TCons ("Flat","Prog") [])])) "FlatXML" "Reads an XML file with a FlatCurry program and returns\nthe FlatCurry program.\n" False UnknownFR),(FunctionInfo "xml2FlatCurry" (FuncType (TCons ("XML","XmlExp") []) (TCons ("Flat","Prog") [])) "FlatXML" "Transforms an XML term into a FlatCurry program.\n" False KnownFlex)] [])